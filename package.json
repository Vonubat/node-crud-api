{
  "name": "node-crud-api",
  "version": "1.0.0",
  "description": "simple CRUD API using in-memory database underneath",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon --exec ts-node-esm src/index.ts",
    "start:prod": "tsc && node ./build/index.js",
    "dev:multi": "nodemon --exec ts-node-esm src/index.ts --balancer",
    "start:multi": "tsc && node ./build/index.js --balancer",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vonubat/node-crud-api.git"
  },
  "keywords": [
    "nodejs"
  ],
  "author": "Vonubat <vonubat@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Vonubat/node-crud-api/issues"
  },
  "homepage": "https://github.com/Vonubat/node-crud-api#readme",
  "dependencies": {
    "@types/uuid": "^9.0.0",
    "dotenv": "^16.0.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "nodemon": "^2.0.20",
    "prettier": "2.8.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
